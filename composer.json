{
    "name": "wentao-php/push-log-to-plume",
    "type": "library",
    "version": "1.0.0",
    "description": "laravel 框架中，监听日志时间将日志传到plume服务中",
    "autoload": {
        "psr-4": {
            "Wentao\\PushLogToPlume\\": "src/"
        }
    },
    "authors": [
        {
            "name": "wentao",
            "email": "taowen@pharmaoryx.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0|^9.0|^10.0",
        "mateusjunges/laravel-kafka": "^1.8",
        "longlang/phpkafka": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wentao\\PushLogToPlume\\Providers\\PushLogToPlumeServiceProvider"
            ]
        }
    }
}
